; i++) {\r\n        pages.push(i)\r\n    }\r\n    return <div>\r\n        <div>\r\n            {pages.map(p => {\r\n                return <span className={(props.currentPage === p) && (styles.activePage)}\r\n                             onClick={(e) => {\r\n                                 props.onPageChange(p)\r\n                             }}>{p}</span>\r\n            })}\r\n        </div>\r\n        {\r\n            props.users.map((u) => <div key={u.id}>\r\n            <span>\r\n            <div>\r\n            <NavLink to={'/profile/' + u.id}>\r\n                <img\r\n                    src={u.photos.small != null ? u.photos.small : 'https://i.pinimg.com/564x/71/c4/f3/71c4f37425bd3775d98bfaedbf349823.jpg'}\r\n                    className={styles.userPhoto}/>\r\n            </NavLink>\r\n            </div>\r\n            <div>\r\n        {u.followed\r\n            ? <button disabled = {props.followingInProgress.some(id => id === u.id)} onClick={() => {\r\n                props.unfollow(u.id)\r\n            }}>Unfollow</button>\r\n            : <button disabled = {props.followingInProgress.some(id => id === u.id)} onClick={() => {\r\n                props.follow(u.id)\r\n            }}>Follow</button>}\r\n\r\n            </div>\r\n            </span>\r\n                <span>\r\n            <span>\r\n            <div>{u.name}</div>\r\n            <div>{u.status}</div>\r\n            </span>\r\n            <span>\r\n            <div>{'u.location.country'}</div>\r\n            <div>{'u.location.city'}</div>\r\n            </span>\r\n            </span>\r\n\r\n\r\n            </div>)\r\n        }\r\n    </div>\r\n}\r\nexport default Users","C:\\Users\\Maria\\my-app\\src\\Components\\Content\\MyPosts\\Post\\Post.jsx",["155"],"import s from './Post.module.css'\r\n\r\n\r\nconst Post = (props) => {\r\n    return (\r\n         <div className = {s.item}>\r\n             <img src = 'https://i.pinimg.com/474x/04/f3/e5/04f3e5693a319427bdea32d626d6a3fd.jpg'/>\r\n       